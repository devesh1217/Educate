{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      },
      {
        "src": "/public/**", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/classes",
        "dest": "/public/classes/index.html"
      },
      {
        "src": "/profile",
        "dest": "/public/profile/index.html"
      },
      {
        "src": "/tarang",
        "dest": "/public/tarang/index.html"
      },
      {
        "src": "/admin",
        "dest": "/public/admin/index.html"
      },
      {
        "src": "/testimonial",
        "dest": "/public/testimonial/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }